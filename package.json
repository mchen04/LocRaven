{
  "name": "locraven",
  "version": "0.1.0",
  "description": "World's first AI-native local business discovery platform",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "email:build": "email build",
    "email:dev": "email dev --dir ./src/features/emails --port 3001",
    "email:export": "email export",
    "stripe:listen": "stripe listen --forward-to=localhost:3000/api/webhooks --project-name=locraven",
    "generate-types": "npx supabase gen types typescript --project-id hmztritmqsscxnjhrvqi --schema public > src/libs/supabase/types.ts",
    "supabase:link": "env-cmd -f ./.env.local supabase link --project-ref hmztritmqsscxnjhrvqi",
    "migration:new": "supabase migration new",
    "migration:up": "supabase migration up --linked --debug && npm run generate-types",
    "migration:squash": "supabase migration squash --linked",
    "cf:build": "opennextjs-cloudflare build",
    "cf:preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf:deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "deploy:functions": "echo 'Use: supabase functions deploy <function-name>'"
  },
  "keywords": [
    "AI search",
    "local business",
    "ChatGPT",
    "Perplexity", 
    "Claude",
    "SEO",
    "AI optimization"
  ],
  "author": "LocRaven Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/LocRaven.git"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.6.5",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@react-email/components": "^0.0.32",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.54.0",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.539.0",
    "next": "^15.5.0",
    "next-route-handler-pipe": "^1.0.5",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-email": "^2.1.6",
    "react-icons": "^5.4.0",
    "resend": "^4.1.1",
    "stripe": "^17.4.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.2",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.1", 
    "autoprefixer": "^10.4.20",
    "env-cmd": "^10.1.0",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.5.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "postcss": "^8.5.6",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "supabase": "^1.226.4",
    "typescript": "^5.9.2",
    "wrangler": "^4.32.0"
  },
  "overrides": {
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.1"
  }
}